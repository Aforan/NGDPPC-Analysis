./generic -b "Greece" "Hungary" >> distances/eu_GR.dis 
./generic -b "Greece" "Ireland" >> distances/eu_GR.dis 
./generic -b "Hungary" "France" >> distances/eu_HU.dis 
./generic -b "Hungary" "Greece" >> distances/eu_HU.dis 
./generic -b "Hungary" "Germany" >> distances/eu_HU.dis 
./generic -b "Hungary" "Ireland" >> distances/eu_HU.dis 
./generic -b "Ireland" "France" >> distances/eu_IR.dis 
./generic -b "Ireland" "Greece" >> distances/eu_IR.dis 
